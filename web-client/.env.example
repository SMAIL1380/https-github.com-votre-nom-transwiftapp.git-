# URL de l'API Backend
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Nom de l'application
NEXT_PUBLIC_APP_NAME=TranswiftApp Website

# Environnement (development, production, staging)
NEXT_PUBLIC_ENV=development

# Version de l'application
NEXT_PUBLIC_VERSION=1.0.0

# Clé API Google Maps pour la géolocalisation et les cartes
# Obtenir une clé sur: https://console.cloud.google.com/
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Clé publique Stripe pour les paiements
# Obtenir une clé sur: https://dashboard.stripe.com/apikeys
NEXT_PUBLIC_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Configuration des médias sociaux
NEXT_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# Configuration Analytics
NEXT_PUBLIC_GA_TRACKING_ID=your_google_analytics_id
